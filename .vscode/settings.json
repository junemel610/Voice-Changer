{
    "workbench.colorTheme": "Visual Studio Dark - C++",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "code-runner.runInTerminal": true,
    "discord.detailsEditing": "Editing {file_name} ",
    "discord.detailsIdling": "Pahinga muna baka mahimatay",
    "discord.lowerDetailsEditing": "Workspace: {workspace} ",
    "discord.lowerDetailsIdling": "Pahinga muna baka mahimatay",
    "discord.largeImageIdling": "Pahinga muna baka mahimatay",
    "discord.lowerDetailsNoWorkspaceFound": "Tinatamad pa",
    "liveshare.audio.joinCallBehavior": "accept",
    "liveshare.audio.startCallOnShare": true,
    "mssql.connections": [],
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "php",
        "blade",
        "ejs",
        "jinja",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "plaintext",
        "markdown",
        "vue",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml",
        "HTML (EEx)",
        "HTML (Eex)",
        "plist"
    ],
    "php.validate.executablePath": "",
    "[python]": {
        "python.defaultInterpreterPath": "C:/Python311/python.exe",
        "python.autoComplete.extraPaths": ["C:/Python311/Lib/site-packages"],
        "python.linting.pylintPath": "C:/Python311/Scripts/pylint",
        "python.linting.flake8Path": "C:/Python311/Scripts/flake8",
        "editor.formatOnType": true
    },
    "redhat.telemetry.enabled": true,
    "diffEditor.hideUnchangedRegions.enabled": true
}
